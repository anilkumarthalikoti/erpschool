		</action>
		<action name="saveBranchDetails" method="saveBranchDetails" class="com.java.erp.webapp.web.setup.BranchDetailsAction"  >
		<result name="branchDetails">jsp/setup/branchdetails.jsp</result>
		</action>
		
		
		 <action name="coursedetails" method="getCourseDetails" class="com.java.erp.webapp.web.setup.CourseDetailsAction"  >
