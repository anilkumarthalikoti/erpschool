		<action name="studentadmissionfields" method="getStudentAdmissionFields" class="com.java.erp.webapp.web.setup.StudentAdmissionFieldsAction"  >
		<result name="studentadmissionfields">jsp/setup/studentadmissionfields.jsp</result>
