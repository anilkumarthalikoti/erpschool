<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.devMode" value="true" />

	<package name="setup" namespace="/" extends="basic-rule">
		<action name="schooldetails" method="getSchoolDetails" class="com.java.erp.webapp.web.setup.SchoolDetailsAction"  >
		<result name="schoolDetails">jsp/setup/schooldetails.jsp</result>
		</action>
		<action name="saveSchoolDetails" method="saveSchoolDetails" class="com.java.erp.webapp.web.setup.SchoolDetailsAction"  >
		<result name="schoolDetails">jsp/setup/schooldetails.jsp</result>
		</action>
		
	 <action name="branchdetails" method="getBranchDetails" class="com.java.erp.webapp.web.setup.BranchDetailsAction"  >
